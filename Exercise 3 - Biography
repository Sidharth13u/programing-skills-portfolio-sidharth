name=input('enter your first and last name' )
age=int(input('enter your age'))
hometown=input('enter your town')


dict_name={
    'name':name,
    'age':age,
    'hometown':hometown}
#Print the values in separate lines using a single print() statement.
print(dict_name['name'])
print(dict_name['age'])
print(dict_name['hometown'])