def is_even_or_odd(number):

    if number %2 ==0:
        print('its is even number')
    else:
        print('its is odd number')
def main():
    try:
        user=int(input('enter the number'))
        result=is_even_or_odd(user)
        print(result)
    except ValueError:
        print('invalid input.pls enter valid integer')
if name == "main":
    main()